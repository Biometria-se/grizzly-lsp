{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "extensionHost",
      "request": "launch",
      "name": "Launch Client",
      "args": [
        "--extensionDevelopmentPath=${workspaceRoot}/client/vscode",
        "${workspaceFolder}/tests/project"
      ],
      "sourceMaps": true,
      "outFiles": [
        "${workspaceRoot}/client/vscode/out/**/*.js"
      ],
      "preLaunchTask": "debug client"
    }
  ]
}
